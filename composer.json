{
    "name": "alexoliverwd/brace",
    "description": "A simple template language written in PHP",
    "keywords": [
        "handlebars",
        "template",
        "html"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Oliver",
            "homepage": "https://github.com/aoliverwd",
            "role": "Lead"
        }
    ],
    "support": {
        "issues": "https://github.com/aoliverwd/brace/issues"
    },
    "prefer-stable": true,
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "files": [
            "src/brace.php"
        ]
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.82"
    }
}
